<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0" configuration="peppolbis-t68-base-1.0">
	<assert>
		<scope>BII3-T68-R022</scope>
	</assert>
	<test>
		<assert>
			<description>Verify no error on correct use.</description>
			<success>BII3-T68-R022</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:CatalogueLine>
				<cbc:MinimumOrderQuantity>10</cbc:MinimumOrderQuantity>
				<cbc:MaximumOrderQuantity>20</cbc:MaximumOrderQuantity>
			</cac:CatalogueLine>

		</Catalogue>
	</test>
	<test>
		<assert>
			<description>Verify error on incorrect use.</description>
			<error>BII3-T68-R022</error>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:CatalogueLine>
				<cbc:MinimumOrderQuantity>20</cbc:MinimumOrderQuantity>
				<cbc:MaximumOrderQuantity>10</cbc:MaximumOrderQuantity>
			</cac:CatalogueLine>

		</Catalogue>
	</test>
</testSet>
