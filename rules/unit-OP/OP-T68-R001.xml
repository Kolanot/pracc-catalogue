<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0" configuration="peppolbis-t68-base-1.0">
	<assert>
		<scope>OP-T68-R001</scope>
	</assert>
	<test>
		<assert>
			<description>No errors when correct identifiers are used.</description>
			<success number="4">OP-T68-R001</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cbc:EndpointID schemeID="BE:CBE"/>
			<cbc:EndpointID schemeID="NO:ORGNR"/>
			<cbc:EndpointID schemeID="DIGST"/>
			<cbc:EndpointID schemeID="ZZZ"/>

		</Catalogue>
	</test>
	<test>
		<assert>
			<description>Trigger errors when incorrect identifiers are used.</description>
			<error>OP-T68-R001</error>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cbc:EndpointID schemeID="NL:ION"/>

		</Catalogue>
	</test>
</testSet>
